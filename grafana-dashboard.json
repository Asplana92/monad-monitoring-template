{
  "id": null,
  "title": "Monad Node Metrics",
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "Node CPU Usage",
      "targets": [
        {
          "expr": "rate(node_cpu_seconds_total[1m])",
          "legendFormat": "{{mode}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Monad RPC Requests",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "RPC"
        }
      ]
    }
  ]
}
